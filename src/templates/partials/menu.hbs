<section id="menu">
  <h2>Menu</h2>
  <p>Please vote for your favorite items!</p>

  <!-- Generate the menu from menu.json with handleBars -->
  <ul>
    {{#each menuData}}
      <li data-id="{{id}}">
        <span class="menu-title">{{title}}</span>
        <span class="menu-count">{{count}}</span>
      </li>
    {{/each}}
  </ul>

</menu>
